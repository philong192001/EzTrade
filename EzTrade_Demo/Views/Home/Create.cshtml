@model DAL.Models.Code

@{
    ViewData["Title"] = "Create";
}

<h2>Create </h2>
<hr />
<div class="row container">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <label class="control-label">Mã Chứng Khoán</label>
                <input asp-for="NameCode" class="form-control" />
            </div>
            <div class="form-group">
                <label class="control-label">Chi tiết mã chứng khoán</label>
                <select asp-for="Id_details">
                    @foreach (var item in ViewBag.lstCodeDetail)
                    {
                        <option name="@item.Id" value="@item.Id">@item.Id</option>
                    }
                </select>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-action="Index">Back to List</a>
</div>